package task;

public class User {
	String name;
	String pw;
	String phone;
	int charge;
	String title;
	public User(String name, String pw, String phone, int charge) {
		super();
		this.name = name;
		this.pw = pw;
		this.phone = phone;
		this.charge = charge;
	}
	public User() {
		super();
	}
	
}
